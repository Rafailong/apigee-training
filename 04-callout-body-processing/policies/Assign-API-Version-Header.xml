<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Assign-API-Version-Header">
    <DisplayName>Assign API Version Header</DisplayName>
    <Properties/>
    <Set>
        <Headers>
            <Header name="api-version">{version}</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>